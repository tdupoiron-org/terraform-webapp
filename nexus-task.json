[
  {
    "name": "${owner}-${appname}-task",
    "image": "${appimage}",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/${owner}-${appname}",
        "awslogs-region": "eu-west-3",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "containerPort": 8081,
        "hostPort": 8081
      }
    ],
    "tags": [
      {
        "key": "Owner",
        "value": "${owner}-${appname}"
      }
    ]
  }
]
